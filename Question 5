using System;
using System.Collections.Generic;

class IntroProgramming

{
    static void Main(String[] args)
    {
        // Just defining our variables

        Console.WriteLine("Enter your value for a: ");
        int a = int.Parse(Console.ReadLine());
        Console.WriteLine("Now enter your value for b: ");
        int b = int.Parse(Console.ReadLine());
        Console.WriteLine("Okay, your range of numbers is (" + a + " , " + b + ").");

        //Then we need to compare them so that we add correctly.

        bool aLarger = (a > b);
        bool bLarger = (b > a);

        Console.WriteLine("Press any button to get the numbers that are divisible by 5 between these two numbers.");
        Console.ReadKey();

        if (bLarger)
        {
            for (int i = a;  i <= b; i++)
            {
                if (i % 5 == 0)
                {
                    Console.Write(" " + i);
                }
            }
        }
        else
        {
            for (int i = b; i <= a; i++)
            {
                if (i % 5 == 0)
                {
                    Console.Write(" " + i);
                }
            }
        }

        


    }
}
