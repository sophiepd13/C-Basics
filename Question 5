using System;
using System.Collections.Generic;

class IntroProgramming

{
    static void Main(String[] args)
    {
        int hexNum = 2013;
        Console.WriteLine("|{0,-10:X}|", hexNum);
        double negFractNum = -1.856;
        Console.WriteLine("|{0,-10:f2}|", negFractNum);
        double fractNum = 1.856;
        Console.WriteLine("|{0,-10:f2}|", fractNum);
    }
}
